{
  "name": "auth_custom_plate",
  "version": "1.0.0",
  "description": "auth-plate",
  "main": "app.ts",
  "engine": {
    "npm": "6.12.0",
    "node": "10.16.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run build --prefix client",
    "server": "nodemon server/app.ts",
    "start": "node server/app.ts",
    "client": "npm run start --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build"
  },
  "author": "Kasyanchyck Gleb",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mongoose": "^5.7.7",
    "@types/multer": "^1.4.3",
    "@types/node": "^12.12.31",
    "@types/request": "^2.48.4",
    "@types/request-promise": "^4.1.46",
    "@types/rimraf": "^3.0.0",
    "@types/socket.io": "^2.1.4",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.2",
    "ts-node": "^8.8.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv-extended": "^2.7.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.5",
    "multer": "^1.4.2",
    "node-sass": "^4.13.1",
    "request-promise": "^4.2.5",
    "rimraf": "^3.0.2",
    "socket.io": "^2.3.0",
    "typescript": "^3.8.3"
  }
}
